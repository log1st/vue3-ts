{
  "name": "urrobot-front",
  "version": "2.4.0",
  "description": "Urrobot SSR edition. Testing version",
  "private": true,
  "scripts": {
    "serve-dev": "cross-env NODE_ENV=development VUE_APP_API_URL=DEPLOYMENT VUE_CLI_BABEL_TRANSPILE_MODULES=true npm run build-ssr-dev && cross-env NODE_ENV=development VUE_APP_API_URL=DEPLOYMENT VUE_CLI_BABEL_TRANSPILE_MODULES=true npm-run-all -p start serve:client",
    "serve": "cross-env NODE_ENV=production VUE_APP_API_URL=DEPLOYMENT VUE_CLI_BABEL_TRANSPILE_MODULES=true npm run build-ssr && cross-env NODE_ENV=production VUE_APP_API_URL=DEPLOYMENT VUE_CLI_BABEL_TRANSPILE_MODULES=true npm-run-all -p start serve:client",
    "serve:client": "vue-cli-service serve",
    "build-ssr": "npm run build-ssr:server -- --trace-warnings --silent && npm run build-ssr:client -- --no-clean --silent",
    "build-ssr-dev" : "npm run build-ssr-dev:serve-dev -- --trace-warnings --silent && npm run build-ssr:client -- --no-clean --silent",
    "build-ssr:client": "vue-cli-service build",
    "build-ssr:server": "cross-env WEBPACK_TARGET=node VUE_APP_API_URL=DEPLOYMENT VUE_CLI_BABEL_TRANSPILE_MODULES=true vue-cli-service build",
    "build-ssr-dev:serve-dev": "cross-env WEBPACK_TARGET=node VUE_APP_API_URL=DEPLOYMENT VUE_CLI_BABEL_TRANSPILE_MODULES=true vue-cli-service build",
    "start": "nodemon server",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@tinymce/tinymce-vue": "^3.2.4",
    "@vuejs-community/vue-directive-mask": "^1.2.4",
    "apexcharts": "^3.24.0",
    "axios": "^0.19.2",
    "blob-util": "^2.0.2",
    "camelcase": "^6.2.0",
    "chart.js": "^2.9.3",
    "core-js": "^3.6.4",
    "csvtojson": "^2.0.10",
    "downloadjs": "^1.4.7",
    "express": "^4.17.1",
    "html-to-pdfmake": "^2.0.2",
    "html2pdf.js": "^0.9.2",
    "http-proxy-middleware": "^0.19.1",
    "js-base64": "^2.5.2",
    "js-sha256": "^0.9.0",
    "js-sha512": "^0.8.0",
    "jsencrypt": "^3.1.0",
    "jspdf": "^1.5.3",
    "jszip": "^3.5.0",
    "k-progress": "^1.5.0",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "nodemon": "^2.0.7",
    "normalize.css": "^8.0.1",
    "pdfmake": "^0.1.65",
    "pdfmake-font-generator": "^1.0.1",
    "qs": "^6.9.1",
    "rubles": "^0.2.0",
    "secure-ls": "^1.2.6",
    "text-mask-addons": "^3.8.0",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "v-calendar": "^2.2.3",
    "v-mask": "^2.2.1",
    "v-tooltip": "^2.0.3",
    "vue": "^2.6.11",
    "vue-agile": "^1.1.3",
    "vue-apexcharts": "^1.6.0",
    "vue-chartjs": "^3.5.0",
    "vue-click-outside": "^1.1.0",
    "vue-client-only": "^2.0.0",
    "vue-csrf": "^2.0.4",
    "vue-delayed-input-mask": "0.0.11",
    "vue-fragment": "^1.5.1",
    "vue-grid-layout": "^2.3.7",
    "vue-js-toggle-button": "^1.3.3",
    "vue-meta": "^1.5.8",
    "vue-multilevel-accordion": "^1.0.8",
    "vue-recaptcha": "^1.3.0",
    "vue-router": "^3.1.5",
    "vue-scrollto": "^2.18.1",
    "vue-select": "^3.10.8",
    "vue-server-renderer": "^2.6.0",
    "vue-text-mask": "^6.1.2",
    "vuedraggable": "^2.23.2",
    "vuex": "^3.1.2",
    "vuex-persistedstate": "^3.1.0",
    "vuex-router-sync": "^5.0.0",
    "webpack-merge": "^5.7.3"
  },
  "devDependencies": {
    "@babel/helper-plugin-utils": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@vue/cli-plugin-babel": "^4.2.0",
    "@vue/cli-plugin-eslint": "^4.2.0",
    "@vue/cli-plugin-router": "^4.2.0",
    "@vue/cli-plugin-vuex": "^4.2.0",
    "@vue/cli-service": "^4.2.0",
    "@vue/eslint-config-standard": "^5.1.0",
    "babel-eslint": "^10.0.3",
    "chalk": "^2.4.2",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.1.2",
    "lint-staged": "^8.1.5",
    "lodash.merge": "^4.6.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "portfinder": "^1.0.20",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.1.0",
    "webpack-node-externals": "^1.7.2"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
